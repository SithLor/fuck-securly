{
  "name": "Bluebook for Students",
  "description": "Required extension for the Bluebook exam player on managed Chromebooks",
  "version": "1.2.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "main.js"
  },
  "permissions": [
    "system.storage",
    "system.display",
    "system.memory",
    "enterprise.hardwarePlatform",
    "enterprise.deviceAttributes"
  ],
  "externally_connectable": {
    "matches": ["https://*.collegeboard.org/*"]
  }
}
